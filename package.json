{
  "name": "ideaboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@pact-foundation/pact": "^5.9.1",
    "axios": "^0.16.2",
    "babel-preset-react": "^6.24.1",
    "immutability-helper": "^2.3.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-transition-group": "^2.2.0"
  },
  "devDependencies": {
    "jest-cli": "^23.0.0",
    "react-scripts": "1.0.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "pactTest": "export NODE_ENV=pactTest && jest --testRegex \"./test/pact/*/.*(.test.pact.js)\" --runInBand --setupFiles ./test/pact/pactSetup.js --setupTestFrameworkScriptFile ./test/pact//pactTestWrapper.js",
    "pactPublish": "node ./test/pact/publish.js"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
